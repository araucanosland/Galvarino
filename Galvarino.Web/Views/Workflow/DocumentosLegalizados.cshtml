@{
    ViewData["Title"] = "Ingreso de Expediente Legalizado";
}

@section breadcumbs{
    <li><a href="#"><i class="demo-pli-home"></i></a></li>
    <li>Etapas</li>
    <li class="active">@ViewData["Title"]</li>
}

@section script{

 <!--Page Script-->
<environment include="Development">
    <script src="~/js/almacenajeComercial.js"></script>
</environment>
<environment exclude="Development">
    <script src="~/js/almacenajeComercial.min.js"></script>
</environment>

}

<div class="row">
    <div class="col-sm-12 table-toolbar-left">                      
       <form id="form-pistoleo-maestro">
                <div class="input-group mar-btm">
                    <span class="input-group-addon"><i class="glyphicon glyphicon-barcode"></i></span>
                    <input class="form-control" placeholder="Pistolear" type="text" id="codigoSeguimiento" autocomplete="off">
                </div>
            </form>
    </div>
</div>
<div class="row">
    <div class="col-sm-12">
        <div class="alert alert-info"><strong>Recuerda!</strong> Debes pistolear los documentos de los expedientes que ya estan legalizados.</div>
    </div>
</div>
<div class="row">
    <table          id="tabla-generica"
                    data-toggle="table"
                    data-search="false"
                    data-show-refresh="false"
                    data-show-toggle="false"
                    data-show-columns="false"
                    data-page-list="[]"
                    data-page-size="80"
                    data-pagination="true" data-show-pagination-switch="false">
        <thead>
            <tr>
                <th data-field="expediente.credito.folioCredito" data-formatter="formatoIncrementor" data-sortable="false">#</th>
                <th data-field="expediente.credito.folioCredito" data-sortable="false">Folio Credito</th>
                <th data-field="expediente.credito.tipoCredito" data-sortable="false" data-formatter="formatoTipoCredito">Tipo Crédito</th>
                <th data-field="expediente.credito.fechaDesembolso" data-sortable="false" data-formatter="formatoFecha">Fecha Colocación</th>
            </tr>
        </thead>
    </table>
</div>