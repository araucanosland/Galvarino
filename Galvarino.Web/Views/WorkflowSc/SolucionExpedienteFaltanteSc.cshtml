@{
    ViewData["Title"] = "Despacho de Expedientes con Reparo a Notaría";
}


@section breadcumbs{
    <li><a href="#"><i class="demo-pli-home"></i></a></li>
    <li>Etapas</li>
    <li class="active">@ViewData["Title"]</li>
}

@section script{

    <!--Page Script-->
    <environment include="Development">
        <script src="~/js/solucionSucursalPagadoraSc.js"></script>
    </environment>
    <environment exclude="Development">
        <script src="~/js/solucionSucursalPagadoraSc.min.js?g=@Guid.NewGuid().ToString()"></script>
    </environment>

}

<div class="row">
    <div class="col-sm-6 table-toolbar-left">
        <div class="btn-group">
            <button class="btn btn-primary" id="btn-generar-generico" type="button"><i class="glyphicon glyphicon-list-alt"></i> Generar Despacho</button>
        </div>
    </div>
</div>
<div class="row">
    <table id="tabla-generica"
           data-toggle="table"
           data-url="/api/wfsc/v1/nomina-setcomplementario/@ProcesoDocumentos.ETAPA_SOLUCION_REPAROS_SUCURSAL_PAGADORA"
           data-search="false"
           data-show-refresh="false"
           data-show-toggle="false"
           data-show-columns="false"
           data-sort-name="fechaEnvioNotaria"
           data-page-list="[]"
           data-page-size="40"
           data-pagination="true"
           data-side-pagination="server"
           data-show-pagination-switch="false"
           data-locale="es-CL">
        <thead>
            <tr>
                <th data-checkbox="true">Resuelto</th>
                <th data-field="reparo" data-formatter="formatoReparoSet">Motivo</th>
                <th data-field="folioCredito" data-sortable="true">Folio Credito</th>
                <th data-field="rutCliente" data-align="center" data-sortable="true">Rut Afiliado</th>
                <th data-field="tipoCredito" data-sortable="true" data-formatter="formatoTipoCredito">Tipo Crédito</th>
                <th data-field="folioCredito" data-align="left" data-sortable="false" data-formatter="formatoListaDocumentosSetComplementarios">Documentos</th>
                <th data-field="fechaDesembolso" data-sortable="true" data-formatter="formatoFecha">Fecha Desembolso</th>
                @*<th data-field="fechaEnvioNotaria" data-align="center" data-sortable="true" data-formatter="formatoFecha">Fecha Envío</th>*@
                @*<th data-field="seguimientoNotaria" data-align="center" data-sortable="true">Folio Envío</th>*@
            </tr>
        </thead>
    </table>
</div>


<canvas id="barcode"></canvas>